package animal;

public class Voar {

	private int possue;

	public boolean getPossue(String possue) {
		if (this.possue == possue) {
			return true;
		} else {
			return false;
		}
	}

//	public String getPossue() {
//		return possue;
//	}

	public void setPossue(int i) {
		this.possue = i;
	}

}

