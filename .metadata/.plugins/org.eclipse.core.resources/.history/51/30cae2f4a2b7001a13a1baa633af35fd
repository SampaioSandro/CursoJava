package animal;

public class GeraPato {

	public static void main(String[] args) {

		Pato gaviao = new Pato();

		gaviao.setAlimenta("bicando");
		gaviao.setComunica("falando");
		gaviao.setLocomove("cantando");
		gaviao.setMembros(2);
		
		System.out.println("gaviao se alimenta " + gaviao.getAlimenta());
		System.out.println("gaviao se comunica " + gaviao.getComunica());
		System.out.println("gaviao se locomove " + gaviao.getLocomove());
		System.out.println("gaviao se possui " + gaviao.getMembros() + " membros");
		
	}

}
