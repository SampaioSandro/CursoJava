//Gerente eh um funcionário, Gerente herda da classe do funcionario
public class Cliente extends Funcionario implements Autenticavel {

	private int senha;

	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}

	public void setSenha(int senha) {
		this.senha = senha;
	}

	public double getBonificacao() {

		System.out.println("Cahamando o método de bonificação do Gerente");
		return super.getBonificacao();
	}

}
