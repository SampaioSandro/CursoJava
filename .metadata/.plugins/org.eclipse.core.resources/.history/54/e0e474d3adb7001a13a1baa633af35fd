package animal;

public class Voar {

	private String possue = "Sim";

	public boolean autentica(String possue) {
		if (this.possue == possue) {
			return true;
		} else {
			return false;
		}
	}

	public String getPossue() {
		return possue;
	}

	public void setPossue(String possue) {
		this.possue = possue;
	}

}

