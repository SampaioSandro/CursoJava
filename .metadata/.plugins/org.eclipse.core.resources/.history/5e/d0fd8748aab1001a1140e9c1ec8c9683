//Gerente eh um funcionário, Gerente herda da classe do funcionario
public class Gerente extends Funcionario{

	private double salario;
	private int senha;

	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}

	public double getBonificacao() {
		return this.salario;
	}

}
