package animal;

public class Voar {

	private int senha = 2222;

	public void autentica(CriaAsas voa) {
		boolean autenticou = voa.autentica(this.senha);
		if (autenticou) {
			System.out.println("Pode entrar no sistema");
		} else {
			System.out.println("Não pode entra no sistema");
		}
	}

}

