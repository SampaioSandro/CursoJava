package animal;

public class GeraPapagaio {

	public static void main(String[] args) {

		Papagaio louro = new Papagaio();

		louro.setAlimenta("bicando");
		louro.setComunica("falando");
		louro.setLocomove("andando");
		louro.setMembros(2);
		
		System.out.println("Gato se alimenta " + louro.getAlimenta());
		System.out.println("Gato se comunica " + louro.getComunica());
		System.out.println("Gato se locomove " + louro.getLocomove());
		System.out.println("Gato se possui " + louro.getMembros() + " membros");
		
	}

}
